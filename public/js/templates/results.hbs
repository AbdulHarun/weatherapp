<div class="display-block">
    {{#each list}}
        <div>
        <h3>{{formatDate dt}}</h3>
        	<img src="http://openweathermap.org/img/w/{{weather.0.icon}}.png" alt="">(Min {{temp.min}}°C - Max {{temp.max}}°C) {{weather.0.main}}
        	<h4>Humidity: {{humidity}}</h4>
        	<h4>Pressure: {{pressure}}</h4>
        </div>
    {{/each}}
</div>